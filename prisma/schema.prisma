generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model users {
  id       Int    @id @unique(map: "sqlite_autoindex_users_1") @default(autoincrement())
  email    String @unique(map: "sqlite_autoindex_users_2")
  password String
}

///
model likes {
  id      Int    @id @default(autoincrement())
  imageId String
  userId  Int
}

///
model history {
  id     Int    @id @default(autoincrement())
  search String
  userId Int
}
